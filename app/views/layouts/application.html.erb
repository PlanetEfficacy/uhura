<!DOCTYPE html>
<html>
  <head>
    <title>Uhura</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= stylesheet_link_tag "http://fonts.googleapis.com/icon?family=Material+Icons" %>
    <%= stylesheet_link_tag "https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.8/css/materialize.min.css" %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= render "shared/page/header" %>
    <% if current_user %>
      <%= redner "shared/small_space" %>
    <% end %>
    <main>
      <%= yield %>
    </main>
    <%= render "shared/page/footer" %>
  </body>

</html>

<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.8/js/materialize.min.js"></script>
